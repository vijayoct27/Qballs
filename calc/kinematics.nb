Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"R", "[", "\[Alpha]_", "]"}], ":=", " ", 
   RowBox[{"{", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"1", ",", "0", ",", "0"}], "}"}], ",", 
     RowBox[{"{", 
      RowBox[{"0", ",", 
       RowBox[{"Cos", "[", "\[Alpha]", "]"}], ",", " ", 
       RowBox[{"-", 
        RowBox[{"Sin", "[", "\[Alpha]", "]"}]}]}], "}"}], ",", 
